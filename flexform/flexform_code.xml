<?xml version="1.0" encoding="iso-8859-1" standalone="yes"?>
<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:syntaxhighlight/language/controller.xml:extlabel</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<label>
						<TCEforms>
							<label>LLL:EXT:syntaxhighlight/language/controller.xml:label</label>
							<config>
								<type>input</type>
								<size>36</size>
								<max>60</max>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</label>
					<language>
						<TCEforms>
							<label>LLL:EXT:syntaxhighlight/language/controller.xml:language</label>
							<config>
								<type>select</type>
								<items>
									<numIndex index="0">
										<numIndex index="0">--</numIndex>
										<numIndex index="1"/>
									</numIndex>
								</items>
								<itemsProcFunc>tx_syntaxhighlightAPI-&gt;getFlexformLanguages</itemsProcFunc>
							</config>
						</TCEforms>
					</language>
					<code>
						<TCEforms>
							<label>LLL:EXT:syntaxhighlight/language/controller.xml:code</label>
							<config>
								<type>text</type>
								<cols>120</cols>
								<rows>12</rows>
							</config>
						</TCEforms>
					</code>
					<linenumbers>
						<TCEforms>
							<label>LLL:EXT:syntaxhighlight/language/controller.xml:linenumbers</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</linenumbers>
					<width>
						<TCEforms>
							<label>LLL:EXT:syntaxhighlight/language/controller.xml:width</label>
							<config>
								<type>input</type>
								<size>4</size>
								<max>4</max>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</width>
					<height>
						<TCEforms>
							<label>LLL:EXT:syntaxhighlight/language/controller.xml:height</label>
							<config>
								<type>input</type>
								<size>4</size>
								<max>4</max>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</height>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>
