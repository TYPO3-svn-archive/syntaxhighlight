<?xml version="1.0" encoding="iso-8859-1" standalone="yes"?>
<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sVIEW>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:syntaxhighlight/controller/locallang.xml:extlabel</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<cLabel>
						<TCEforms>
							<label>LLL:EXT:syntaxhighlight/controller/locallang.xml:label</label>
							<config>
								<type>input</type>
								<size>36</size>
								<max>60</max>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</cLabel>
					<cLang>
						<TCEforms>
							<label>LLL:EXT:syntaxhighlight/controller/locallang.xml:lang</label>
							<config>
								<type>select</type>
								<items>
									<numIndex index="0">
										<numIndex index="0">--</numIndex>
										<numIndex index="1"/>
									</numIndex>
								</items>
								<itemsProcFunc>tx_syntaxhighlightAPI-&gt;getFlexformLanguages</itemsProcFunc>
							</config>
						</TCEforms>
					</cLang>
					<cCode>
						<TCEforms>
							<label>LLL:EXT:syntaxhighlight/controller/locallang.xml:code</label>
							<config>
								<type>text</type>
								<cols>120</cols>
								<rows>12</rows>
							</config>
						</TCEforms>
					</cCode>
					<cNumbers>
						<TCEforms>
							<label>LLL:EXT:syntaxhighlight/controller/locallang.xml:numbers</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</cNumbers>
					<cWidth>
						<TCEforms>
							<label>LLL:EXT:syntaxhighlight/controller/locallang.xml:width</label>
							<config>
								<type>input</type>
								<size>4</size>
								<max>4</max>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</cWidth>
					<cHeight>
						<TCEforms>
							<label>LLL:EXT:syntaxhighlight/controller/locallang.xml:height</label>
							<config>
								<type>input</type>
								<size>4</size>
								<max>4</max>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</cHeight>
				</el>
			</ROOT>
		</sVIEW>
	</sheets>
</T3DataStructure>
