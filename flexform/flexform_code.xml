<?xml version="1.0" encoding="iso-8859-1" standalone="yes"?>
<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:syntaxhighlight/language/controller.xml:extlabel</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<label>
						<TCEforms>
							<label>LLL:EXT:syntaxhighlight/language/controller.xml:label</label>
							<config>
								<type>input</type>
								<size>36</size>
								<max>60</max>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</label>
					<labelMode>
						<TCEforms>
							<label>LLL:EXT:syntaxhighlight/language/controller.xml:labelMode</label>
							<config>
								<type>select</type>
								<itemsProcFunc>tx_syntaxhighlightAPI-&gt;getFlexformLabelMode</itemsProcFunc>
								<items>
									<numIndex index="0">
										<numIndex index="0">LLL:EXT:syntaxhighlight/language/controller.xml:labelMode.0</numIndex>
										<numIndex index="1">language</numIndex>
									</numIndex>
									<numIndex index="1">
										<numIndex index="0">LLL:EXT:syntaxhighlight/language/controller.xml:labelMode.1</numIndex>
										<numIndex index="1">label</numIndex>
									</numIndex>
									<numIndex index="2">
										<numIndex index="0">LLL:EXT:syntaxhighlight/language/controller.xml:labelMode.2</numIndex>
										<numIndex index="1">none</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</labelMode>
					<language>
						<TCEforms>
							<label>LLL:EXT:syntaxhighlight/language/controller.xml:language</label>
							<config>
								<type>select</type>
								
								<itemsProcFunc>tx_syntaxhighlightAPI-&gt;getFlexformLanguages</itemsProcFunc>
							</config>
						</TCEforms>
					</language>
					<code>
						<TCEforms>
							<label>LLL:EXT:syntaxhighlight/language/controller.xml:code</label>
							<config>
								<type>text</type>
								<cols>120</cols>
								<rows>12</rows>
							</config>
						</TCEforms>
					</code>
					<lineNumbers>
						<TCEforms>
							<label>LLL:EXT:syntaxhighlight/language/controller.xml:lineNumbers</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</lineNumbers>
					<alternateLines>
						<TCEforms>
							<label>LLL:EXT:syntaxhighlight/language/controller.xml:alternateLines</label>
							<config>
								<type>input</type>
								<size>4</size>
								<max>10</max>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</alternateLines>
					<startLine>
						<TCEforms>
							<label>LLL:EXT:syntaxhighlight/language/controller.xml:startLine</label>
							<config>
								<type>input</type>
								<size>4</size>
								<max>10</max>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</startLine>
					<width>
						<TCEforms>
							<label>LLL:EXT:syntaxhighlight/language/controller.xml:width</label>
							<config>
								<type>input</type>
								<size>4</size>
								<max>4</max>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</width>
					<height>
						<TCEforms>
							<label>LLL:EXT:syntaxhighlight/language/controller.xml:height</label>
							<config>
								<type>input</type>
								<size>4</size>
								<max>4</max>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</height>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>
